<?xml version="1.0"?>
<object name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="openmultimedia.imagewatchdog"
    purge="False">
    <configlet title="Image WatchDog settings"
        action_id="openmultimedia.imagewatchdog.settings"
        appId="openmultimedia.imagewatchdog"
        category="Products"
        condition_expr=""
